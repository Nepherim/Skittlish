omments, group, page%0a[@%0a(:if false:)[[#meta-data-foot]](:if:)%0a(:div9999 class="footer":)%0a(:if [ !equal {$$comments} "{$bi_CommentType_NONE}" && equal {$bi_CommentsEnabled} true ] :)\%0a%25comments%25[[{$$group}.{$$page}#commentblock | \%0a(:includesection "#comments-count-pagelist entrygroup='{$$group}' entryname='{$$page}' commentstatus=true":) comment(s)]]%25%25\%0a(:if:)%0a!!!!%25meta%25(:if !equal "{$$entrytags}" "":)tags: (:blogit tags:){$$entrytags}(:blogitend:)(:if:)%0a(:div9999end:)%0a[[#meta-data-footend]]%0a@]%0a%0a----%0a!! Comments%0a!!! #comment-view%0a[@%0a(:if false:)[[#comment-view]](:if:)%0a(:template first:)%0a(:if equal {$$onlyunapproved} "false":)%0a!!!%25block content%25$[Comments]%0a(:if:)%0a(:template each:)%0a(:if [ equal {$$onlyunapproved} "true" && !equal {(bi_base {=$FullName})} {(bi_base {%3c$FullName})} ]:)%0a!!! [[{(bi_base {=$FullName})} | {{(bi_base {=$FullName})}$Title}]](:if:)%0a* %25apply=item class=comment%25\%0a  %25meta%25'''(:if [ equal "{$bi_LinkToCommentSite}" "true" && !equal "{=$:website}" "" ] :)[[{=$:website} | {=$:commentauthor}]](:else:){=$:commentauthor}(:if:)''' on ''{(ftime fmt="%25a, %25d %25b %25Y @%25H:%25M" when=@{=$:commentdate})}''\%0a(:if [ auth admin && equal {=$:commentapproved} false ] :)\%0a | [[{*$FullName}?action=blogitapprove&pn={=$FullName} | $[approve]]](:if:)\%0a(:if auth admin:) | [[{=$FullName}?action=edit | $[edit]]](:if:)%0a  %25body%25(:blogit multiline:){=$:commenttext}(:blogitend:)%25%25%0a[[#comment-viewend]]%0a@]%0a%0a!!! #comment-form%0a[@%0a(:if false:)[[#comment-form]](:if:)%0a!!!%25block content%25$[Leave a comment]%0a(:messages:)%0a(:fieldset:)%0a:$[Name] $[(required)]:(:input text $:commentauthor id="comment-author":)%0a:$[E-mail] $[(required, will not be published)]:(:input text $:email id="comment-email":)%0a:$[Website]:(:input text $:website id="comment-website":)%0a:$[Comment]:(:input textarea $:commenttext rows=10 cols=50 id="comment-text":)%0a(:if equal {$EnablePostCaptch